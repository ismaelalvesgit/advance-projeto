{
  "name": "backend-advance",
  "version": "0.0.1",
  "author": "Ismael Alves <cearaismael1997@gmail.com>",
  "main": "index.js",
  "scripts": {
    "start": "babel-node index.js",
    "dev": "cross-env ENVIROMENT=DEV nodemon --exec babel-node index.js",
    "test": "cross-env ENVIROMENT=TEST DB_CACHE_ENABLE=false babel-node jest.startup.js --forceExit --detectOpenHandles --maxWorkers=1 --testTimeout=50000 --coverage",
    "secure-mode": "cross-env ENVIROMENT=TEST DB_CACHE_ENABLE=false babel-node jest.startup.js --forceExit --detectOpenHandles --watchAll --maxWorkers=1 --testTimeout=50000",
    "doc": "apidoc -i doc/ -o src/public/documentacao",
    "snyk-protect": "snyk protect",
    "prepare": "npm run snyk-protect",
    "docker:build": "docker build -t backend .",
    "docker:run": "docker run -d -p 3000:3000 --name backend backend",
    "docker:down": "docker stop backend"
  },
  "husky": {
    "hooks": {
      "pre-push": "npm test"
    }
  },
  "keywords": [],
  "license": "ISC",
  "dependencies": {
    "body-parser": "^1.19.0",
    "compression": "^1.7.4",
    "date-fns": "^2.12.0",
    "connect-multiparty": "^2.2.0",
    "cors": "^2.8.5",
    "ejs": "^3.1.2",
    "express": "^4.17.1",
    "express-validator": "^6.4.0",
    "frameguard": "^3.1.0",
    "fs": "0.0.1-security",
    "helmet": "^3.22.0",
    "helmet-csp": "^2.10.0",
    "hide-powered-by": "^1.1.0",
    "hsts": "^2.2.0",
    "ienoopen": "^1.1.0",
    "ioredis": "^4.17.3",
    "mongodb": "^3.5.6",
    "mongoose": "^5.9.10",
    "morgan": "^1.10.0",
    "nodemailer": "^6.4.6",
    "prom-client": "^12.0.0",
    "rate-limiter-flexible": "^2.1.6",
    "response-time": "^2.3.2",
    "speedtest-net": "^1.6.2",
    "uuid": "^3.4.0",
    "shelljs": "^0.8.4",
    "mv": "^2.1.1",
    "rimraf": "^3.0.2",
    "apidoc": "^0.19.1",
    "x-xss-protection": "^1.3.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.7.4",
    "@babel/core": "^7.7.4",
    "@babel/node": "^7.7.4",
    "@babel/plugin-proposal-class-properties": "^7.7.4",
    "@babel/plugin-proposal-nullish-coalescing-operator": "^7.7.4",
    "@babel/plugin-transform-async-to-generator": "^7.7.4",
    "@babel/plugin-transform-runtime": "^7.7.4",
    "@babel/polyfill": "^7.7.0",
    "@babel/preset-env": "^7.7.4",
    "@babel/runtime": "^7.7.4",
    "babel-jest": "^26.0.1",
    "cross-env": "^7.0.2",
    "husky": "^3.1.0",
    "jest": "^26.0.1",
    "nodemon": "^2.0.1",
    "snyk": "^1.339.1",
    "supertest": "^4.0.2"
  },
  "snyk": true
}
